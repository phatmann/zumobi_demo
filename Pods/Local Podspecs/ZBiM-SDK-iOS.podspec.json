    "MessageUI",
    "PassKit",
    "Security",
    "Social",
    "StoreKit",
    "SystemConfiguration",
    "Twitter",
    "UIKit"
  ],
  "libraries": [
    "sqlite3",
    "z"
  ],
  "dependencies": {
    "Bolts": [
      "~> 1.1.0"
    ],
    "Mantle": [
      "~> 1.4"
    ],
    "TMCache": [
      "~> 1.2.1"
    ],
    "XMLDictionary": [
      "~> 1.4.0"
    ],
    "UICKeyChainStore": [
      "~> 1.0"
    ],
    "Reachability": [
      "~> 3.1"
    ],
    "GZIP": [
      "~> 1.0.3"
    ],
    "FMDB": [
      "~> 2.4.0"
    ]
  }
